<% provide(:title, @user.name) %>
<div class="row">
  <div class="col-xs-6 col-md-4 form-body">
    <%= render 'user' %>
  </div>
  <div class="col-xs-12 col-sm-6 col-md-8">
    <ul class="nav nav-tabs">
      <%= render 'user_head' %>
    </ul>
    <ul>
      <% @topics.each do |topic| %>
        <li><%= topic.title %></li>
      <% end %>
    </ul>
  </div>
</div>
